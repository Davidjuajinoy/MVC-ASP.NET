@model IEnumerable<OdeToFood.Data.Models.Restaurant>
@{
    ViewBag.Title = "Home Page";
}

<div class="jumbotron">
    <h1>ASP.NET</h1>
    @*<p class="lead">Hay  @Enumerable.Count(Model)  restaurantes en la base de datos.</p>*@

    <p class="lead">Hay  @Model.Count() restaurantes en la base de datos.</p>

</div>

<ul>
    @foreach (var restaurante in Model)
    {
        <li>@restaurante.Name</li>
    }
</ul>

